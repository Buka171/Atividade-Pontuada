programa {
  funcao inicio() {
    //declarando variáveis
    cadeia nome, quant, preco, total, desconto
    //consultando dados
    escreva("Digite a descrição do produto: ")
    leia(nome)

    escreva("Digite a quantidade adquirida: ")
    leia(quant)

    escreva("Digite o preço unitário: ")
    leia(preco)
    //calculo
    total == quant * preco
    
    //exibindo
    se(quant <= 5) {desconto 2 * }
    se(quant > 5 e quant <=10) {desconto 3%}
    se(quant > 10 o desconto 5%.)
  }
}
